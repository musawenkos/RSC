{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]

  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "AzureAd": {
    "Instance": "",
    "Domain": "",
    "TenantId": "",
    "ClientId": "",
    "ClientSecret": "",
    "CallbackPath": ""
  },
  "MicrosoftGraph": {
    "BaseUrl": "",
    "Scopes": ""
  },
  "PathBase": "/RoadSignCapture",
  "Redis": {
    "Host": "",
    "Port": ""
  },
  "Elasticsearch": {
    "Uri": "",
    "Username": "",
    "Password": ""
  }
}